{
  "schema": {
    "version": 0,
    "meta": {
      "flags": [
        "experimentalData"
      ]
    },
    "collections": {
      "post": {
        "name": "post",
        "label": "Posts",
        "path": "src/content/blog",
        "format": "mdx",
        "fields": [
          {
            "name": "title",
            "label": "Title",
            "type": "string",
