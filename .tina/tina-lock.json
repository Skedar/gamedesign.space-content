            "namespace": [
              "post",
              "title"
            ]
          },
          {
            "name": "description",
            "label": "Description",
            "type": "string",
            "required": true,
            "namespace": [
              "post",
              "description"
            ]
          },
          {
            "name": "pubDatetime",
            "label": "Publication Date",
            "type": "datetime",
            "required": true,
            "namespace": [
              "post",
              "pubDatetime"
            ]
          },
          {
            "name": "author",
            "label": "Author",
            "type": "string",
            "required": true,
            "namespace": [
              "post",
              "author"
            ]
          },
          {
            "name": "tags",
            "label": "Tags",
            "type": "string",
            "required": true,
            "list": true,
            "namespace": [
              "post",
              "tags"
            ]
          },
          {
            "name": "body",
            "label": "Body",
